<sdk-bundle>
  <daemon-table>
    <default-package>sync-hellopics-apps-mgmt</default-package>

    <daemon-entry>
      <name>hellopics-mgmt</name>
      <ui-name>sync-hellopics-mgmt-service</ui-name>
      <binary>INSTALLDIR/sbin/hellopics-mgmt</binary>
      <description>SDK Your Net Corp. Hellopics example management process</description>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <!-- runs on both master and backup REs when installed on both -->
      <mode-protocol-master/>
      <mode-protocol-backup/>
      <run-if-configured/>
      <noversion/>
    </daemon-entry>

  </daemon-table>
</sdk-bundle>

