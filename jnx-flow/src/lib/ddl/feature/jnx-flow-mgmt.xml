<sdk-bundle>
  <daemon-table>
    <default-package>jnx-flow-mgmt</default-package>

    <daemon-entry>
      <name>jnx-flow-mgmt</name>
      <ui-name>jnx-flow-service</ui-name>
      <description>Sample flow service process</description>
      <binary>INSTALLDIR/sbin/jnx-flow-mgmt</binary>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <mode-protocol-master/>
      <run-if-configured/>
    </daemon-entry>

  </daemon-table>

  <require-table>

    <require-entry>
      <name>jnx-flow-mgmt</name>
      <help>Juniper sample flow configuration</help>
      <configure/>
      <view/>
    </require-entry>
  </require-table>

</sdk-bundle>
