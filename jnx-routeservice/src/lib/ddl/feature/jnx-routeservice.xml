<sdk-bundle>
  <daemon-table>
    <default-package>jnx-routeservice</default-package>

    <daemon-entry>
      <name>jnx-routeserviced</name>
      <ui-name>jnx-routeservice</ui-name>
      <description>Basic route add/delete services</description>
      <binary>INSTALLDIR/sbin/jnx-routeserviced</binary>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <mode-protocol-master/>
      <run-if-configured/>
    </daemon-entry>

  </daemon-table>

  <require-table>

    <require-entry>
      <name>jnx-routeservice</name>
      <help>jnx-routeservice configuration</help>
      <configure/>
      <view/>
    </require-entry>
  </require-table>

</sdk-bundle>
