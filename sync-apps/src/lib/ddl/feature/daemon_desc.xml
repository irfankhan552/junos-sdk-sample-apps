<sdk-bundle>
  <daemon-table>
    <default-package>sync-apps</default-package>

    <daemon-entry>
      <name>helloworldd</name>
      <ui-name>sync-helloworld-service</ui-name>
      <binary>INSTALLDIR/sbin/helloworldd</binary>
      <description>SDK Your Net Corp. Helloworld Example process</description>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <mode-protocol-master/>
      <run-if-configured/>
      <noversion/>
    </daemon-entry>
    
    <daemon-entry>
      <name>counterd</name>
      <ui-name>sync-counter-service</ui-name>
      <binary>INSTALLDIR/sbin/counterd</binary>
      <description>SDK Your Net Corp. Counter Example process</description>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <mode-protocol-master/>
      <run-if-configured/>
      <noversion/>
    </daemon-entry>

  </daemon-table>

</sdk-bundle>

