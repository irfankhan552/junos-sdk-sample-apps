<pkg-config>

    <!-- Package configuration header - one per config file -->
    <header>
        <!-- config file syntax version -->
        <version>1.0</version>
        
        <!-- unique services package (container) name -->
        <name>sync-passthru-plugin</name>
    </header>
 
    <!-- List of plugins -->
    <plug-in>
        <name>passthru-plugin</name>
        
        <!-- plugin shared object full name (with path) -->
        <path>/opt/sdk/lib/libpassthru-plugin.so.1</path>
        
        <!-- plugin entry point; function is called when plugin is loaded -->
        <entry>passthru_entry</entry>
    </plug-in>

</pkg-config>
