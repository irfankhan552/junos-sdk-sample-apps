<sdk-bundle>
  <daemon-table>
    <default-package>jnx-msprsm</default-package>

    <daemon-entry>
      <name>jnx-msprsmd</name>
      <ui-name>jnx-msprsm</ui-name>
      <description>MSP Redundant Set Manager</description>
      <binary>INSTALLDIR/sbin/jnx-msprsmd</binary>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <mode-protocol-master/>
      <run-if-configured/>
    </daemon-entry>

  </daemon-table>

  <require-table>

    <require-entry>
      <name>jnx-msprsm</name>
      <help>MSP Redundant Set Manager configuration</help>
      <configure/>
      <view/>
    </require-entry>
  </require-table>

</sdk-bundle>
