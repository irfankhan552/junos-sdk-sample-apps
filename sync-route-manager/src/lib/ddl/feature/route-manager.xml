<sdk-bundle>

    <daemon-table>
        <default-package>route-manager</default-package>

        <daemon-entry>
            <name>route-manager</name>
            <ui-name>route-manager</ui-name>
            <binary>INSTALLDIR/sbin/route-manager</binary>
            <description>SDK Your Net Corp. Route Manager process</description>
            <system-process-failover acceptable="exclude"/>
            <optional/>
            <checkconf/>
            <mode-protocol-master/>
            <run-if-configured/>
            <noversion/>
        </daemon-entry>

    </daemon-table>

    <require-table>
        <require-entry>
            <name>route-manager</name>
            <help>SDK Your Net Co. Route Manager configuration</help>
            <configure/>
            <view/>
        </require-entry>
    </require-table>

</sdk-bundle>

