<sdk-bundle>
  <daemon-table>
    <default-package>jnx-ifinfo</default-package>

    <daemon-entry>
      <name>jnx-ifinfod</name>
      <ui-name>jnx-ifinfo-service</ui-name>
      <description>ifinfo service process</description>
      <binary>INSTALLDIR/sbin/jnx-ifinfod</binary>
      <system-process-failover acceptable="exclude"/>
      <optional/>
      <checkconf/>
      <mode-protocol-master/>
      <run-if-configured/>
    </daemon-entry>

  </daemon-table>

  <require-table>

    <require-entry>
      <name>jnx-ifinfo</name>
      <help>Interface info configuration</help>
      <configure/>
      <view/>
    </require-entry>
  </require-table>

</sdk-bundle>
